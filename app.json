{
  "expo": {
    "name": "quick-note",
    "slug": "quick-note",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "assets/images/favicon.svg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "assets/images/favicon.svg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "assets/images/favicon.svg",
        "backgroundColor": "#1F1F1F"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "assets/images/favicon.svgg"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
